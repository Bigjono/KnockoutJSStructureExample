

@model dynamic

@{
    ViewBag.Title = "title";
}
 



<table>
    <thead>
        <tr>
            <th>#</th>
            <th>Name</th>
            <th>Mobile</th>
        </tr>
    </thead>
    
    <tbody data-bind="foreach: staffMembers">
        <tr>
            <td data-bind="value: id"></td>
            <td data-bind="value: name"></td>
            <td data-bind="value: mobile"></td>
        </tr>
    </tbody>

</table>



@section scripts
{
     
    <script src="~/scripts/app/staff_main.js" type="text/javascript"></script>

}
